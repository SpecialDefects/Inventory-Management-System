      </Label>
      <ButtonBar layoutX="200.0" layoutY="28.0" prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <RadioButton fx:id="inHouseButton" mnemonicParsing="false" onAction="#onActionInHouseButton" selected="true" text="In-House" />
            <RadioButton fx:id="outsourcedButton" layoutX="127.0" layoutY="22.0" mnemonicParsing="false" onAction="#onActionOutsourcedButton" text="Outsourced" />
