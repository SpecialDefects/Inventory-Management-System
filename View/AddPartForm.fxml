            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="partName" layoutX="225.0" layoutY="142.0" />
      <TextField fx:id="partInv" layoutX="225.0" layoutY="175.0" />
      <TextField fx:id="partCost" layoutX="225.0" layoutY="208.0" />
      <TextField fx:id="partMax" layoutX="225.0" layoutY="241.0" />
      <TextField fx:id="partMin" layoutX="427.0" layoutY="241.0" />
      <Button fx:id="save" layoutX="380.0" layoutY="326.0" mnemonicParsing="false" onAction="#onActionSavePart" text="Save" />
      <Button fx:id="cancel" layoutX="437.0" layoutY="326.0" mnemonicParsing="false" onAction="#onActionCancel" text="Cancel" />
      <Label layoutX="105.0" layoutY="111.0" text="ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="105.0" layoutY="145.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="105.0" layoutY="178.0" text="Inv">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="105.0" layoutY="211.0" text="Price/Cost">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="105.0" layoutY="244.0" text="Max">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="389.0" layoutY="244.0" text="Min">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="partTypeValue" layoutX="225.0" layoutY="274.0" />
      <Label fx:id="partTypeLabel" layoutX="105.0" layoutY="277.0" text="Machine ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
