<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddPartFormController">
   <children>
      <Label layoutX="37.0" layoutY="34.0" text="Add Part">
         <font>
            <Font name="System Bold" size="18.0" />
